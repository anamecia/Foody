<ul>
    <%@events.each do |event|%>
    <%=link_to event.title, event_path(event.id)%>
      <p>Description: <%=event.description%></p>
      <p>Date: <%=event.date%></p>
      <p>Time: <%=event.time%></p>
      <p>Unsold Places: <%=event.places%></p>
      <p>Price: <%=event.price%></p>
    <%end%>
</ul>